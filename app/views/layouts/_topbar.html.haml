.navbar.navbar-fixed-top
  .navbar-inner
    = link_to 'TurnChannel', '#', :class => 'brand'
    %ul.nav
      %li.active= link_to 'Dubstep', '#', :class => 'genre-link', :id => 'dubstep'
      %li= link_to 'DrumNBass', '#', :class => 'genre-link', :id => 'dnb'
      %li= link_to 'Electro', '#', :class => 'genre-link', :id => 'electro'
    %ul.nav.pull-right
      %li= link_to 'Submit Tracks', '#', :id => 'submit-tracks-link'
      %li= link_to 'Submit Art', '#', :id => 'submit-art-link'
      %li= link_to 'Contact', '#', :id => 'contact-link'
    #social-links
      .social
        %a{:href=>"https://twitter.com/TurnChannel", :class=>"twitter-follow-button", :'data-show-count'=>"false"} Follow @TurnChannel
      .social
        .fb-like{:'data-href'=>"http://www.facebook.com/TurnChannel", :'data-send'=>"false", :'data-layout'=>"button_count", :'data-width'=>"450", :'data-show-faces'=>"false"}

      :javascript
        !function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="//platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");
